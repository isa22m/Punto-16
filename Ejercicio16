# Leer un número entero positivo introducido por el usuario
numero = int(input("Introduce un número entero positivo: "))

# Crear una lista para almacenar los números impares
impares = []

# Encontrar todos los números impares desde 1 hasta el número introducido
for i in range(1, numero + 1):
    if i % 2 != 0:
        impares.append(i)

# Mostrar los números impares separados por comas
print(", ".join(map(str, impares)))
